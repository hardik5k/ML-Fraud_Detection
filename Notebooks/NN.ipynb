    "def accuracy(y_p, y):\n",
    "    _, preds = torch.max(y_p, dim=1)\n",
    "   # print(y_p)\n",
    "    return torch.tensor(torch.sum(preds == y).item() / len(preds))\n",
    "#     return roc_auc_score(y, preds)"
